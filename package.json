{
	"name": "cards-view",
	"version": "1.9.3-beta.1",
	"description": "Plugin for Obsidian.md. Displays a card view of your notes.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"prebuild": "npm run lezer",
		"postinstall": "npm run lezer",
		"lezer": "lezer-generator src/search/search.grammar -o src/search/grammar.js",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint && prettier --check **/*.mjs **/*.ts **/*.svelte && svelte-check",
		"test": "node --test --require tsx/cjs $(find . -name \"*.test.ts\")"
	},
	"keywords": [],
	"author": "Maud Royer <hello@maudroyer.fr>",
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "^9.16.0",
		"@tsconfig/svelte": "^5.0.2",
		"@types/masonry-layout": "^4.2.7",
		"@types/minimasonry": "^1.3.5",
		"@types/node": "^24.10.4",
		"@typescript-eslint/eslint-plugin": "^8.18.0",
		"@typescript-eslint/parser": "^8.18.0",
		"builtin-modules": "^5.0.0",
		"esbuild": "^0.27.1",
		"esbuild-svelte": "^0.9.4",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"lezer-generator": "^0.13.4",
		"obsidian": "latest",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.3.4",
		"svelte-eslint-parser": "^1.4.1",
		"svelte-preprocess": "^6.0.3",
		"tsafe": "^1.8.5",
		"tslib": "^2.6.2",
		"tsx": "^4.19.2",
		"typescript": "^5.5.0",
		"typescript-eslint": "^8.18.0",
		"vitest": "^4.0.15"
	},
	"dependencies": {
		"minimasonry": "^1.3.2"
	}
}
